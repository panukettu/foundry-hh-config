{
  "name": "@panukresko/easy-foundryup",
  "version": "0.2.1",
  "description": "Install and manage foundryup",
  "homepage": "https://github.com/foundry-rs/hardhat",
  "repository": "github:foundry-rs/hardhat",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "anvil",
    "foundry",
    "foundryup"
  ],
  "scripts": {
    "install-foundry": "ts-node ./src/foundryup.ts",
    "lint": "yarn prettier --check && yarn eslint",
    "lint:fix": "yarn prettier --write && yarn eslint --fix",
    "eslint": "eslint 'src/**/*.ts'",
    "prettier": "prettier \"**/*.{js,md}\"",
    "build": "tsc --build .",
    "clean": "rimraf dist",
    "test": "mocha --recursive \"test/**/*.ts\" --exit"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "command-exists": "^1.2.9",
    "ts-interface-checker": "^0.1.9"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/debug": "^4.1.4",
    "@types/fs-extra": "^5.1.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "chai": "^4.3.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.1",
    "eslint-plugin-prettier": "3.4.0",
    "mocha": "^10.0.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "4.7.2"
  }
}
